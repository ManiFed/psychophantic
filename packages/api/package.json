{
  "name": "@psychophant/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && prisma db push --skip-generate && tsc",
    "deploy": "prisma db push && npm run build",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "db:push": "prisma db push",
    "db:push:accept": "prisma db push --accept-data-loss",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.2",
    "@prisma/client": "^6.2.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.34.8",
    "fastify": "^5.2.1",
    "ioredis": "^5.4.2",
    "stripe": "^20.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.6",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
